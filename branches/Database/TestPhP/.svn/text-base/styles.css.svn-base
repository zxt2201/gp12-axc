table 
{ 
	background-color: white;
	border: 1px solid black;
	margin: auto;
	border-collapse: collapse;
	text-align:left;
}

tr.tablehead 
{ 
	border: 1px solid black;
	font-weight: bold;
}

td
{
	border: 1px solid;
	border-top: 0px;
	border-bottom: 0px;
	padding: 0px;
	padding-left:6px;
	padding-right:6px;
}

body
{
	margin:0px;
	background-color: #FFFFFF;
	color:#000000;
	font-family: verdana, sans-serif, verdana, arial, serif;
	font-size:14px;
	text-align:center;
}

h1
{
	margin:20px;
	font-size:32px;
	text-align:center;
	font-weight:bold;
}

div.content
{
	margin:auto;
	background-color: #FFFFFF;
	padding:10px;
}

div.subcontent
{
    margin:auto;
	margin-bottom:40px;
	width: 700px;
	background-color: #FFFFFF;
	border-color: #000000;
	border: 2px solid;
}

div.subcontentheader
{
	background-image:url('./images/transblend.png');
	background-repeat:repeat-x;
	background-position:top;
	font-size:22px;
	background-color:#000000;
	text-align:center;
	font-weight:bold;
	height:80px;
}

span.textcenter
{
	position: relative;
	top: 20px;
}

span.textleft
{
	position: relative;
	float:left;
	top: 12px;
	left:12px;
	font-family:  sans-serif, verdana, arial, serif;
	font-size:36px;
	font-size:36px;
	text-align:center;
	font-weight:normal;
}

span.imageright
{
	position: relative;
	float:right;
	width: 133px;
	height: 50px;
	background-image:url('./images/logos.png');
	background-repeat:no-repeat;
	background-position:center;
	top: 10px;
	right:10px
}

div.subcontentbody
{
	font-family: sans-serif, verdana, arial, serif;
	padding: 0px 20px 20px 20px;
}

input.search
{
	width: 400px;
}



